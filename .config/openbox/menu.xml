<?xml version="1.0" encoding="UTF-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
  <menu id="apps-accessories-menu" label="Accessories">
    <item label="Htop">
      <action name="Execute">
        <command>terminator -T htop -e htop</command>
      </action>
    </item>
    <item label="Iftop">
      <action name="Execute">
        <command>sudo terminator -T iftop -e iftop</command>
        <startupnotify>
          <enabled>yes </enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Iotop">
      <action name="Execute">
        <command>terminator -T iotop -e iotop</command>
      </action>
    </item>
    <item label="Ntop">
      <action name="Execute">
        <command>terminator -T ntop -e ntop</command>
      </action>
    </item>
    <item label="Remmina">
      <action name="Execute">
        <command>remmina</command>
      </action>
    </item>
  </menu>
  <menu id="apps-editors-menu" label="Editors">
    <item label="VSCode">
      <action name="Execute">
        <command>code</command>
        <startupnotify>
          <enabled>yes </enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Intellij">
      <action name="Execute">
        <command>idea</command>
        <startupnotify>
          <enabled>yes </enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Neovim">
      <action name="Execute">
        <command>terminator -T neovim -e nvim</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>GVim</wmclass>
        </startupnotify>
      </action>
    </item>
    <item label="Emacs">
      <action name="Execute">
        <command>emacs</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>Emacs</wmclass>
        </startupnotify>
      </action>
    </item>
  </menu>
  <menu id="apps-term-menu" label="Terminals">
    <item label="Alacritty">
      <action name="Execute">
        <command>alacritty</command>
      </action>
    </item>
    <item label="Rxvt Unicode">
      <action name="Execute">
        <command>urxvt</command>
      </action>
    </item>
    <item label="Terminator">
      <action name="Execute">
        <command>terminator</command>
      </action>
    </item>
    <item label="Xterm">
      <action name="Execute">
        <command>xterm</command>
      </action>
    </item>
  </menu>
  <menu id="apps-net-menu" label="Internet">
    <item label="Chromium">
      <action name="Execute">
        <command>chromium</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>Chromium</wmclass>
        </startupnotify>
      </action>
    </item>
    <item label="Elinks">
      <action name="Execute">
        <command>terminator -T elinks -e elinks</command>
        <startupnotify>
          <enabled>yes </enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Firefox">
      <action name="Execute">
        <command>firefox</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>Firefox</wmclass>
        </startupnotify>
      </action>
    </item>
    <item label="Rainbowstream">
      <action name="Execute">
        <command>terminator -T rainbowstream -e rainbowstream</command>
        <startupnotify>
          <enabled>yes </enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Pidgin Instant Messenger">
      <action name="Execute">
        <command>pidgin</command>
        <startupnotify>
          <enabled>yes</enabled>
        </startupnotify>
      </action>
    </item>
  </menu>
  <menu id="apps-office-menu" label="Office">
    <item label="Evince">
      <action name="Execute">
        <command>evince</command>
      </action>
    </item>
    <item label="OpenOffice Base">
      <action name="Execute">
        <command>/opt/openoffice4/program/sbase</command>
      </action>
    </item>
    <item label="OpenOffice Calc">
      <action name="Execute">
        <command>/opt/openoffice4/program/scalc</command>
      </action>
    </item>
    <item label="OpenOffice Draw">
      <action name="Execute">
        <command>/opt/openoffice4/program/sdraw</command>
      </action>
    </item>
    <item label="OpenOffice Impress">
      <action name="Execute">
        <command>/opt/openoffice4/program/simpress</command>
      </action>
    </item>
    <item label="OpenOffice Math">
      <action name="Execute">
        <command>/opt/openoffice4/program/smath</command>
      </action>
    </item>
    <item label="OpenOffice Writer">
      <action name="Execute">
        <command>/opt/openoffice4/program/swriter</command>
      </action>
    </item>
  </menu>
  <menu id="apps-multimedia-menu" label="Multimedia">
    <item label="MPlayer">
      <action name="Execute">
        <command>mplayer</command>
        <startupnotify>
          <enabled>yes</enabled>
          <wmclass>MPlayer</wmclass>
        </startupnotify>
      </action>
    </item>
    <item label="MPSyt">
      <action name="Execute">
        <command>terminator -T mpsyt -e mpsyt</command>
      </action>
    </item>
    <item label="VLC">
      <action name="Execute">
        <command>vlc</command>
        <startupnotify>
          <enabled>yes</enabled>
        </startupnotify>
      </action>
    </item>
  </menu>
  <menu id="apps-fileman-menu" label="File Managers">
    <item label="PCMan File Manager">
      <action name="Execute">
        <command>pcmanfm-qt</command>
        <startupnotify>
          <enabled>yes</enabled>
        </startupnotify>
      </action>
    </item>
  </menu>
  <menu id="apps-graphics-menu" label="Graphics">
    <item label="Gimp">
      <action name="Execute">
        <command>gimp</command>
        <startupnotify>
          <enabled>yes</enabled>
        </startupnotify>
      </action>
    </item>
  </menu>
  <menu id="system-menu" label="System">
    <item label="Openbox Menu">
      <action name="Execute">
        <command>obmenu3</command>
      </action>
    </item>
    <item label="Openbox Configuration Manager">
      <action name="Execute">
        <command>obconf</command>
        <startupnotify>
          <enabled>yes</enabled>
        </startupnotify>
      </action>
    </item>
    <item label="Manage Cups Printers">
      <action name="Execute">
        <command>xdg-open http://localhost:631/</command>
        <startupnotify>
          <enabled>no</enabled>
          <icon>cups</icon>
        </startupnotify>
      </action>
    </item>
    <separator/>
    <item label="Reconfigure Openbox">
      <action name="Reconfigure"/>
    </item>
  </menu>
  <menu id="root-menu" label="Openbox 3">
    <separator label="Applications"/>
    <menu id="apps-accessories-menu"/>
    <menu id="apps-editors-menu"/>
    <menu id="apps-graphics-menu"/>
    <menu id="apps-net-menu"/>
    <menu id="apps-office-menu"/>
    <menu id="apps-multimedia-menu"/>
    <menu id="apps-term-menu"/>
    <menu id="apps-fileman-menu"/>
    <separator label="System"/>
    <menu id="system-menu"/>
    <separator/>
    <item label="Log Out">
      <action name="Exit">
        <prompt>yes</prompt>
      </action>
    </item>
  </menu>
</openbox_menu>
